<Group labelreference="L256759" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font style="Text">Function to generate the library</Font>
GEN := proc(BASE)
  local filename;
  global savelibname;
  filename := cat(DIR, BASE, &quot;.mla&quot;);
  march('create', filename);
  savelibname := filename;
  savelibname := savelib(BASE);
end proc:
</Text-field>
</Input>
