<Group view="code" labelreference="L551" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GEN := proc( MODULE_NAME )
  uses FT=FileTools;
  local bin_library_path, src_library_path, ds := kernelopts('dirsep');
  bin_library_path := cat(BINARY_DIR,ds,&quot;bin&quot;,ds,MODULE_NAME,&quot;.mla&quot;);
  src_library_path := cat(SOURCE_DIR,ds,MODULE_NAME,&quot;.mpl&quot;);
  if FT:-Exists(src_library_path) then
    if not FT:-Exists(cat(BINARY_DIR,ds,&quot;bin&quot;)) then
      FT:-MakeDirectory(cat(BINARY_DIR,ds,&quot;bin&quot;));
    end:
    if FT:-Exists(bin_library_path) then
      FT:-Remove(bin_library_path):
    end:
    read(src_library_path):
    march('create', bin_library_path ):
    savelib(convert(MODULE_NAME,symbol), bin_library_path ):
  else
<Font encoding="UTF-8">    error &quot;Missing file %1\134n&quot;, </Font>src_library_path;
  end:
end proc:</Text-field>
