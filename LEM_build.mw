<Group view="code" hide-input="false" labelreference="L684" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># https://www.mapleprimes.com/questions/230637-How-To-Copy-Directory-In-Maple
CopyDir := proc(src::string, dest::string)
  uses FT=FileTools;
  local n:= 1+length(src), r, d, f, limb, ds := kernelopts('dirsep');
  if not FT:-Exists(dest) then FT:-MakeDirectory(dest) fi;
  for r in FT:-Walk(src, 'topdown', 'followlinks') do
    limb:= cat(dest, ds, r:-dir[n..], ds);
    for d in r:-subdirs do
      (d-&gt; `if`(FT:-Exists(d), 0, FT:-MakeDirectory(d)))(cat(limb, d));
    od;
    seq(FT:-Copy(cat(r:-dir, ds, f), cat(limb, f)), f= r:-files) 
  od;
  return;
end proc:</Text-field>
</Input>
</Group></Presentation-Block><Presentation-Block>
